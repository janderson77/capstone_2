{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "user_id": 1,
            "game_id": 2,
            "in_game_name": "tacocat"
        }
    ],
    "required": [
        "user_id",
        "game_id",
        "in_game_name"
    ],
    "properties": {
        "user_id": {
            "$id": "#/properties/user_id",
            "type": "integer",
            "title": "The user_id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "game_id": {
            "$id": "#/properties/game_id",
            "type": "integer",
            "title": "The game_id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                2
            ]
        },
        "in_game_name": {
            "$id": "#/properties/in_game_name",
            "type": "string",
            "title": "The in_game_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "tacocat"
            ]
        }
    },
    "additionalProperties": false
}